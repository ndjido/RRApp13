<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="fr" lang="fr">

<head>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
      
    <!-- favicon -->
    <link rel="shortcut icon" type="image/png" href="images/favicon.png" />
    <!-- EOF favicon -->
     
      <!-- Main css -->
    <%= stylesheet_link_tag    "css/style", :media => "all" %>
      
      <!-- Bootstrap css -->
    <%= stylesheet_link_tag    "bootstrap/css/bootstrap", :media => "all" %>
    <%= stylesheet_link_tag    "bootstrap/css/bootstrap-responsive", :media => "all" %>
    
	
        <!-- Bootstrap JS -->
    <%= javascript_include_tag    "bootstrap/js/bootstrap-transition"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-alert"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-modal"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-scrollspy"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-tab"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-tooltip"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-popover"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-button"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-collapse"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-carousel"%>
    <%= javascript_include_tag    "bootstrap/js/bootstrap-typeahead"%>
    	<!-- EOF Bootstrap JS -->
              
</head>
<!-- EOF head -->
